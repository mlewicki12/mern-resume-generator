{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "no-console": {
      "severity": "warning",
      "options": [
        "log",
        "error",
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "no-shadowed-variable": {
      "severity": "warning"
    }
  },
  "rulesDirectory": []
}